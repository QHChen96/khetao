<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.khetao.serve.product.product.mapper.TagMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.khetao.serve.product.entity.Tag">
        <id column="id" property="id" />
        <result column="alias" property="alias" />
        <result column="shop_id" property="shopId" />
        <result column="tag_name" property="tagName" />
        <result column="status" property="status" />
        <result column="tag_type" property="tagType" />
        <result column="item_count" property="itemCount" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modify" property="gmtModify" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, alias, shop_id, tag_name, status, tag_type, item_count, gmt_create, gmt_modify
    </sql>

</mapper>
